{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [],
            "cStandard": "c17",
            "intelliSenseMode": "linux-gcc-x64",
            "cppStandard": "c++23",
            "compilerPath": "/usr/bin/g++",
            "configurationProvider": "ms-vscode.makefile-tools",
            "compilerArgs": [
                "-g ",
                "-O3 ",
                "-Wpedantic ",
                "-Wall ",
                "-Wextra ",
                "-Wmisleading-indentation ",
                "-Wunused ",
                "-Wuninitialized ",
                "-Wshadow ",
                "-Wconversion ",
                "-Werror ",
                "-std=c++23"
            ]
        }
    ],
    "version": 4
}